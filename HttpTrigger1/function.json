{
  "scriptFile": "test.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "blob",
      "direction": "out",
      "name": "outputblob",
      "path": "scrapefile/scrape.csv",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name":"inputblob",
      "type":"blob",
      "direction":"in",
      "path":"scrapefile/*.csv",
      "connection":"AzureWebJobsStorage"
    },
    {
      "name":"copyblob",
      "type":"blob",
      "direction":"out",
      "path":"scrapefile/old/{name}-{date}.csv",
      "connection":"AzureWebJobsStorage"
    }
  ]
}